#include <iostream>
# include <string>
using namespace std;



int main() {
   string nome;
    double salario;

    cout << "Qual o seu nome? " << endl;
    getline (cin, nome);
    cout << "Qual o seu salario? ";
    cin >> salario;
    cout << "o funcionario," <<nome<< " tem um salario de "  <<salario;
   return 0;
}
